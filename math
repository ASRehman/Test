class abc{

public static void main (Strring args[]){
int a=10;
int b=20;
int c;
c=a+b;
System.out.println(c);
c=a-b;
System.out.println(c);
c=a*b;
System.out.println(c);
c=a/b;
System.out.println(c);
c=a*a;
System.out.println(c);
c=power(a,b);
System.out.println(c);
c=sqrt(c);
System.out.println(c);
}
}
